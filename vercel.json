{
    "version": 2,
    "public": true,
    "builds": [
      {
        "src": "package.json",
        "use": "@vercel/static-build",
        "config": {
          "distDir": "dist"
        }
      }
    ],
    "rewrites": [
      {
        "source": "/task/:path*",
        "destination": "https://tasky-rm-backend.vercel.app/task/:path*"
      },
      {
        "source": "/:path*",
        "destination": "https://tasky-rm-backend.vercel.app/:path*"
      }
    ],
    "staticFiles": {
      "public": "public"
    }
  }
  