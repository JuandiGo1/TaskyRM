{
    "version": 2,
    "public": true,
    "builds": [
      {
        "src": "package.json",
        "use": "@vercel/static-build",
        "config": {
          "distDir": "dist"
        }
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "/index.html"
      }
    ],
    "rewrites": [
      {
        "source": "/task/:path*",
        "destination": "https://taskyrm-backend.up.railway.app/task/:path*"
      },
      {
        "source": "/:path*",
        "destination": "https://taskyrm-backend.up.railway.app/:path*"
      }
    ]
  }
  