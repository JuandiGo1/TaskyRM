{
    "version": 2,
    "public": true,
    "builds": [
      {
        "src": "frontend/index.html",
        "use": "@vercel/static",
        "config": {
        "distDir": "frontend"
      }
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "frontend/index.html"
      }
    ]
      
    }
    